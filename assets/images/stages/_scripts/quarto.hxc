function create() {
    //dadPos.x -= 100;
    //dadCam.y += 150;

    bfCam.y += 100;
    gfCam.y -= 180;
    gfCam.x -= 200;

    this.gfVersion = "no-gf";
    this.camZoom = 0.83;

    var bg = new FlxSprite(0, -70).loadGraphic(Paths.image("stages/quarto"));
    bg.scale.set(1.5,1.5);
    add(bg);
}